{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Schedules": {
    "NamespaceScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "ServiceScheduler": {
      "enabled": "true",
      "schedule": "0 */5 * * * ?"
    },
    "DeploymentScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "HealthCheckScheduler": {
      "enabled": "true",
      "schedule": "0 */5 * * * ?"
    }
  },
  "Watchers": {
    "NamespaceWatcher": {
      "enabled": "true",
      "resetSchedule": "0 */1 * * * ?",
      "keyFormat": ""
    }
  },
  "queue": {
    "servicev1": "healthcheck.service-local",
    "scale": "deployment.scale-local"
  }
}