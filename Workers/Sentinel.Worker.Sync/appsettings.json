{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Schedules": {
    "NamespaceScheduler": "true",
    "NamespaceSchedule": "0 */5 * * * ?",
    "ServiceScheduler": "true",
    "ServiceSchedule": "0 */5 * * * ?",
    "DeploymentScheduler": "true",
    "DeploymentSchedule": "0 */5 * * * ?",
    "HealthCheckScheduler": "true",
    "HealthCheckSchedule": "0 */5 * * * ?"
  }
}