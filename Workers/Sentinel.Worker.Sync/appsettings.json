{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Schedules": {
    "NamespaceScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "ServiceScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "HealthCheckScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentScalerScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    }
  },
  "Watchers": {
    "NamespaceWatcher": {
      "enabled": "true",
      "resetSchedule": "0 */1 * * * ?",
      "keyFormat": ""
    },
    "DeploymentWatcher": {
      "enabled": "true",
      "resetSchedule": "0 */1 * * * ?",
      "keyFormat": ""
    }
  },
  "queue": {
    "healthcheckStatusUpdate": "healthcheck.status.update"
  }
}