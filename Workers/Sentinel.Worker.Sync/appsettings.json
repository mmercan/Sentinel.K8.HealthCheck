{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Schedules": {
    "NamespaceSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "ServicesSynccheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "HealthCheckSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentScalerSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    }
  },
  "Watchers": {
    "NamespaceWatcher": {
      "enabled": "true",
      "resetSchedule": "0 */1 * * * ?",
      "keyFormat": ""
    },
    "DeploymentWatcher": {
      "enabled": "true",
      "resetSchedule": "0 */1 * * * ?",
      "keyFormat": ""
    }
  },
  "queue": {
    "healthcheckStatusUpdate": "healthcheck.status.update"
  }
}