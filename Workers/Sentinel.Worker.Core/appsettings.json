{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Schedules": {
    "NamespaceSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "ServicesSynccheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "HealthCheckSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    },
    "DeploymentScalerSyncScheduler": {
      "enabled": "true",
      "schedule": "0 */1 * * * ?"
    }
  },
  "queue": {
    "healthcheckStatusUpdate": "healthcheck.status.update"
  }
}